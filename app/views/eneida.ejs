<!DOCTYPE html>
<html>
  <head>
    <title><%= siteTitle%> <%= pageTitle%></title>
    <link rel="stylesheet" type="text/css" href="/style.css">
  </head>
  <body >
    <div class="outer">
      <div class="inner">
       <ul> 
      <% allEneida.forEach(function(item) { %>
       <% if (pageId == 'eneida'){ %><li>
        <a href="/eneida/<%= item.row %>"> 
          <%- item.text %>
        </a>
        </li>
         <% } %>
         <% if (pageId == 'eneidaRow'){ %><li>
          <%- item.text %>
        </li>
        <li><a class="back" href="/eneida/">До тексту </a></li>
         <% } %>
       <% }); %>
     </ul>
    </div>
  </div>

  </body>
</html>
